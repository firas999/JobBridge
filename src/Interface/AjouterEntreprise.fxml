<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AjouterEntrepriseController">
   <children>
      <VBox layoutX="177.0" layoutY="68.0" prefHeight="249.0" prefWidth="88.0" spacing="15.0">
         <children>
            <Label text="Nom entreprise" />
            <Label text="Secteur" />
            <Label text="Matricule fiscal" />
            <Label text="Site " />
            <Label text="Email" />
            <Label text="Telephone" />
            <Label text="Taille" />
         </children>
      </VBox>
      <VBox layoutX="285.0" layoutY="68.0" prefHeight="200.0" prefWidth="139.0" spacing="7.0">
         <children>
            <TextField fx:id="tfNomEntreprise" />
            <TextField fx:id="tfSecteurEntreprise" layoutX="10.0" layoutY="35.0" />
            <TextField fx:id="tfMatriculeFiscal" layoutX="10.0" layoutY="60.0" />
            <TextField fx:id="tfSiteEntreprise" layoutX="10.0" layoutY="85.0" />
            <TextField fx:id="tfEmailEntreprise" layoutX="10.0" layoutY="110.0" />
            <TextField fx:id="tfTelephoneEntreprise" layoutX="10.0" layoutY="135.0" />
            <TextField fx:id="tftailleEntreprise" layoutX="10.0" layoutY="160.0" />
         </children>
      </VBox>
      <Button fx:id="btnInsert" layoutX="248.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleButtonAction" onMouseClicked="#addEntreprise" text="Inserer" />
      <Label layoutX="251.0" layoutY="31.0" text="Ajouter entreprise" />
      <Button fx:id="loadImage" layoutX="299.0" layoutY="305.0" mnemonicParsing="false" onAction="#loadIMG" text="Button" />
      <ImageView fx:id="LogoEntreprise" fitHeight="150.0" fitWidth="200.0" layoutX="386.0" layoutY="242.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
